    <class>Entity.PaymentMaster</class>
    <class>Entity.Booking</class>
    <class>Entity.Hostel</class>
    <class>Entity.CategoryMaster</class>
    <class>Entity.PaymentDetail</class>
    <class>Entity.StateMaster</class>
